=== function addWoundLevelBit(mask, woundLevel)
{
	- woundLevel == 1:
	 ~addBit( mask, WOUND_BIT_LEVEL_1)
	- woundLevel == 2:
	 ~addBit( mask, WOUND_BIT_LEVEL_2)
	- woundLevel == 3:
	 ~addBit( mask, WOUND_BIT_LEVEL_3)
	- woundLevel == 4:
	 ~addBit( mask, WOUND_BIT_LEVEL_4)
	- woundLevel == 5:
	 ~addBit( mask, WOUND_BIT_LEVEL_5)
	- else:
		~elseResulted = 1
}



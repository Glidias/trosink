=== function applyWound(applyPain,  applyBL,  applyShock,  ref charPartShock, ref charPartPain, ref charPartBL )
{
	- applyShock  == -1:	
		~charPartShock = 999
}
{
	- applyPain  == -1:	
		~charPartPain = 999
}
{
	- applyShock > 0 && applyShock > charPartShock:
		~charPartShock = applyShock
}
{
	- applyPain > 0 && applyPain > charPartPain:
		~charPartPain = applyPain
}
{
	- applyBL > charPartBL:
		~charPartBL = applyBL
}


=== function getPartWoundDamages(targetPart, woundLevel, damageType, ref applyPain, ref applyBL, ref applyShock, wp)
{
	///* woundLocations
	

	- targetPart == "foot": 
	{
		

		-damageType== DAMAGE_TYPE_CUTTING:
			{
			

			- woundLevel == 1:
				~applyPain = 2 - 1*wp 
				~applyBL =   0
				~applyShock =  3 - 1*wp 
			

			- woundLevel == 2:
				~applyPain = 3 - 1*wp 
				~applyBL =   1
				~applyShock =  3 - 0*wp 
			

			- woundLevel == 3:
				~applyPain = 5 - 1*wp 
				~applyBL =   2
				~applyShock =  4 - 0*wp 
			

			- woundLevel == 4:
				~applyPain = 6 - 1*wp 
				~applyBL =   5
				~applyShock =  6 - 0*wp 
			

